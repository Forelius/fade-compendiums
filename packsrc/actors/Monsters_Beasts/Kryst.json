      "_originalKey": "!actors.items!zUrq2sYPpyIWRLHQ.IW5EeSPPymdwjgXA",
      "folder": "OR1oqiLvw7LWXY42",
      "name": "Dragon Breath",
      "type": "specialAbility",
      "img": "systems/fantastic-depths/assets/img/item/specialAbility.png",

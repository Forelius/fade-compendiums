  "!items!cDelNgr5HTdbMfjR": {
    "name": "Magic's End",
    "type": "spell",
    "img": "systems/fantastic-depths/assets/img/item/spell.png",
    "folder": "TnEmaPQavdbpoj5v",
    "system": {
      "tags": [
        "bard"
      ],
      "description": "<p>When sung, this spell-song dispels all spells and magical effects within its area of effect. However, unlike the <em>Dispel Magic</em> spell, there is always a chance that a spell or effect will resist being dispelled.</p><p><strong>Effects:</strong></p><ul><li>Dispels all spells and magical effects within a 20' x 20' x 20' cube.</li><li>Each affected spell or effect has a base 20% resistance if its level is equal to or lower than the bard's level.</li><li>For every level higher than the bard's, the resistance increases by +5%.</li></ul><p><strong>Resistance Calculation:</strong></p><ul><li>Example: A 7th-level bard attempts to dispel a <em>Cloudkill</em> spell cast by a 12th-level magic-user.</li><li>The resistance chance is 20% + (5% x 5) = 45%.</li><li>Roll [[/r 1d100 #Dispel Resistance]]; if the result is lower than or equal to the resistance chance, the spell resists being dispelled.</li></ul><p><strong>Additional Notes:</strong></p><ul><li>Applies to spell-like effects from magic items, with the DM estimating the original caster's level.</li><li>Permanent magical effects are subject to DM discretion for dispelling.</li></ul>",
      "gm": {
        "notes": ""
      },
      "spellLevel": 4,
      "range": "60'",
      "duration": "Permanent",
      "effect": "Attempts to dispel all magical effects in the area",
      "targetSelf": false,
      "targetOther": true,
      "durationFormula": "",
